{
  "name": "ReactNativeApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "dev": "ENVFILE=.env.development react-native run-android && react-native start",
    "stage": "ENVFILE=.env.staging react-native run-android && react-native start",
    "prod": "ENVFILE=.env.production react-native run-android",
    "apk-dev": "cd android && ENVFILE=.env.development ./gradlew assembleRelease",
    "apk-stage": "cd android && ENVFILE=.env.staging ./gradlew assembleRelease",
    "apk-prod": "cd android && ENVFILE=.env.production ./gradlew assembleRelease",
    "test": "jest",
    "lint": "eslint .",
    "postinstall": "npx jetify"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.13.2",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-community/netinfo": "^5.9.5",
    "axios": "0.19.2",
    "install": "0.13.0",
    "npm": "6.14.5",
    "react": "16.11.0",
    "react-native": "0.63.3",
    "react-native-config": "^1.3.1",
    "react-native-firebase": "5.6.0",
    "react-native-gesture-handler": "1.6.1",
    "react-native-image-crop-picker": "^0.32.2",
    "react-native-image-picker": "^2.3.3",
    "react-native-indicators": "^0.17.0",
    "react-native-reanimated": "1.9.0",
    "react-native-root-toast": "^3.2.1",
    "react-native-safe-area-context": "3.0.7",
    "react-native-screens": "2.9.0",
    "react-redux": "7.2.0",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.10.3",
    "@babel/runtime": "7.10.3",
    "@react-native-community/eslint-config": "0.0.5",
    "babel-jest": "24.9.0",
    "eslint": "6.8.0",
    "jest": "24.9.0",
    "metro-react-native-babel-preset": "0.58.0",
    "react-test-renderer": "16.11.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
